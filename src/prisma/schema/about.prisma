model About {
  id           String    @id @default(auto()) @map("_id") @db.ObjectId
  version      Float     @default(0.1) @map("__v")
  title        String
  introduction String
  createdAt    DateTime  @default(now()) @db.Date
  updatedAt    DateTime  @updatedAt @db.Date
  deletedAt    DateTime? @db.Date
}
