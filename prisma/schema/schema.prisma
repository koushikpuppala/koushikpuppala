generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder", "strictUndefinedChecks"]
  output          = "../../.prisma"
}

datasource db {
  provider     = "mongodb"
  relationMode = "prisma"
  url          = env("PRISMA_URI")
  directUrl    = env("MONGO_URI")
}
